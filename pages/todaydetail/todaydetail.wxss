view {
  margin-left: 5px;
  margin-right: 5px;
}
.content {
  display: flex;
  background-color: #FF9999;
  border-radius: 25px;
  margin-top: 8px;
}
.content view text {
  color: #FFFFFF;
  font-size: 14px;
}
.content view text .bold {
  font-weight: 600;
  font-size: 15px;
}
.content view {
  margin-left: 10px;
}
.img-box {
  display: flex;
  justify-content: center;
  align-items: center;
}
.nav {
  display: flex;
}
.nav-content {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  height: 60px;
  width: 25%;
  margin-right: 5px;
  margin-top: 10px;
  background-color: #FF9999;
  color: #FFFFFF;
  border-radius: 40px;
}
.line-box {
  margin-top: 10px;
}
.line-box text {
  font-size: 16px;
  font-weight: 600;
}
.line{
  width: 98%;
  margin-right: 5px;
  border-top:1px #989898 solid;
}